{
  "name": "styleboard",
  "version": "0.3.3",
  "homepage": "https://github.com/mnmlstme/styleboard",
  "authors": [
    "ken@mnmlst.me"
  ],
  "description": "Documentation and test framework for CSS",
  "main": "dist",
  "keywords": [
    "CSS",
    "styleguide"
  ],
  "license": "BSD",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "marked": "chjj/marked#0.3.2",
    "mkay": "fgnass/mkay",
    "modernizr": "~2.8.3",
    "prism": "LeaVerou/prism#gh-pages"
  },
  "exportsOverride": {
    "marked": {
      "js": "lib/*.js"
    },
    "mkay": {
        "js": "mkay.js"
    },
    "modernizr": {
        "js": "modernizr.js"
    },
    "prism": {
      "js": "prism.js",
      "css": "themes/prism.css"
    }
  }
}
