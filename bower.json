{
  "name": "styleboard",
  "version": "0.2.4",
  "homepage": "https://github.com/mnmlstme/styleboard",
  "authors": [
    "ken@mnmlst.me"
  ],
  "description": "Documentation and test framework for CSS",
  "main": "dist",
  "keywords": [
    "CSS",
    "styleguide"
  ],
  "license": "BSD",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],
  "dependencies": {
    "marked": "chjj/marked#0.3.2",
    "mkay": "fgnass/mkay",
    "prism": "LeaVerou/prism#gh-pages",
    "requirejs": "2.1.4"
  },
  "exportsOverride": {
    "marked": {
      "js": "lib/*.js"
    },
    "mkay": {
      "js": "mkay.js"
    },
    "prism": {
      "js": "prism.js",
      "css": "themes/prism.css"
    },
    "requirejs": {
      "js": "require.js"
    }
  }
}
